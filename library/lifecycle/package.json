{
  "main": "lib/_export_all_in_once_index.js",
  "monorepo": true,
  "name": "@idlebox/lifecycle",
  "typings": "docs/package-public.d.ts",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc -p src && export-all-in-one src/tsconfig.json",
    "prepare": "im-single-dog",
    "prepublish": "rimraf lib dist && npm run build",
    "upgrade": "ncu --upgrade --packageFile ./package.json",
    "watch": "tsc -w -p src",
    "yarn-publish": "im-single-dog && yarn publish --registry https://registry.npmjs.org --access=public"
  },
  "dependencies": {
    "@idlebox/errors": "~1.0.2"
  },
  "devDependencies": {
    "@gongt/single-dog": "^2.0.2",
    "@idlebox/export-all-in-one": "^3.0.2",
    "@microsoft/api-extractor": "^7.2.2",
    "@types/node": "^12.0.12",
    "npm-check-updates": "^3.1.13",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.2"
  }
}
