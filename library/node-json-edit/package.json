{
	"name": "@idlebox/node-json-edit",
	"description": "Edit json file and preserve it's indent and line feed and final new line.",
	"version": "1.0.5",
	"scripts": {
		"build": "tsc -p src",
		"prepare": "im-single-dog",
		"prepublish": "rimraf lib dist && npm run build",
		"upgrade": "ncu --upgrade --packageFile ./package.json",
		"watch": "tsc -w -p src",
		"yarn-publish": "im-single-dog && yarn publish --registry https://registry.npmjs.org --access=public"
	},
	"main": "index.js",
	"typings": "./lib/index.d.ts",
	"repository": {
		"type": "github",
		"url": "gongt/node-json-edit"
	},
	"license": "MIT",
	"devDependencies": {
		"@gongt/single-dog": "^2.0.2",
		"@microsoft/api-extractor": "^7.2.2",
		"@types/node": "^12.0.12",
		"npm-check-updates": "^3.1.13",
		"rimraf": "^2.6.3",
		"source-map-support": "^0.5.12",
		"typescript": "^3.5.2"
	},
	"dependencies": {
		"source-map-support": "^0.5.12"
	},
	"monorepo": true
}