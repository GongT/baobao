{
	"name": "@gongt/fix-esm",
	"type": "commonjs",
	"version": "1.1.0",
	"description": "wrapper of fix-esm, only register once.",
	"license": "WTFPL",
	"main": "./lib/index.js",
	"module": "./empty.mjs",
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./lib/index.js",
			"default": "./empty.mjs"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepack": "rimraf lib && tsc -p src",
		"build": "tsc -p src",
		"_phase:build": "tsc -p src",
		"watch": "tsc -p src -w",
		"clean": "rimraf lib"
	},
	"dependencies": {
		"esbuild": "^0.24.0",
		"rimraf": "^6.0.1"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@types/node": "^22.7.4"
	}
}
