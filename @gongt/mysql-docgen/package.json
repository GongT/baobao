{
	"name": "@gongt/mysql-docgen",
	"type": "module",
	"version": "1.1.23",
	"description": "生成MySQL文档，用于我自己的微服务系统。",
	"license": "MIT",
	"main": "lib/api.cjs",
	"module": "lib/api.js",
	"types": "./lib/api.d.ts",
	"exports": {
		".": {
			"types": "./lib/api.d.ts",
			"import": "./lib/api.js",
			"require": "./lib/api.cjs"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"mysql-docgenerate": "bin/mysql-docgen.cjs"
	},
	"scripts": {
		"prepack": "heft --unmanaged build --production --clean",
		"build": "heft --unmanaged build",
		"_phase:build": "heft --unmanaged build",
		"watch": "heft --unmanaged build-watch",
		"clean": "heft clean"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/GongT/baobao"
	},
	"dependencies": {
		"@idlebox/common": "workspace:^",
		"fancy-log": "^2.0.0",
		"mysql": "^2.18.1",
		"pretty": "^2.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"source-map-support": "^0.5.21",
		"tslib": "^2.4.1"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@rushstack/heft": "^0.56.0",
		"@types/fancy-log": "^2.0.0",
		"@types/mysql": "^2.15.21",
		"@types/node": "^18.11.0",
		"@types/pretty": "^2.0.1",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.9",
		"@internal/local-rig": "workspace:^"
	},
	"exportAllInOne": {
		"exportEverything": false
	}
}
