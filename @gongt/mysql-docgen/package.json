		"mysql": "^2.18.1",
		"pretty": "^2.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"source-map-support": "^0.5.21",
		"tslib": "^2.4.1"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:*",
		"@microsoft/api-extractor": "^7.33.6",
		"@rushstack/heft": "^0.48.8",
		"@types/fancy-log": "^2.0.0",
		"@types/fs-extra": "^9.0.13",
		"@types/mysql": "^2.15.21",
		"@types/node": "^18.11.0",
		"@types/pretty": "^2.0.1",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.9",
		"local-rig": "workspace:*",
		"typescript": "^4.8.4"
	},
	"exportAllInOne": {
		"exportEverything": false
