{
	"name": "vscode-remote-thief",
	"description": "Run code from remote machine, like from integrated terminal.",
	"private": true,
	"version": "0.0.1",
	"license": "MIT",
	"displayName": "Remote Thief",
	"publisher": "GongT",
	"repository": "https://github.com/GongT/baobao.git",
	"engines": {
		"vscode": "^1.32.0"
	},
	"keywords": ["remote"],
	"categories": ["Other"],
	"extensionKind": ["workspace"],
	"activationEvents": ["*"],
	"main": "./lib/extension.js",
	"contributes": {
		"configuration": {
			"title": "Remote Thief",
			"properties": {
				"remote.thief.binaryPath": {
					"type": "string",
					"default": "",
					"description": "Path to place vscode executable wrapper"
				},
				"remote.thief.binaryName": {
					"type": "string",
					"default": "code",
					"description": "callable name of vscode"
				}
			}
		},
		"commands": [
			{
				"command": "extension.steal",
				"title": "Steal code binary"
			}
		]
	},
	"scripts": {
		"watch": "tsc -w -p src",
		"build": "tsc -p src",
		"clean": "rimraf lib"
	},
	"dependencies": {
		"tslib": "^2.0.1",
		"@gongt/vscode-helpers": "^0.0.3"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@build-script/single-dog-asset": "^1.0.6",
		"@types/node": "^14.10.2",
		"typescript": "^4.0.2"
	}
}
