{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ESNext",
		"rootDir": ".",
		"outDir": "../lib",
		"noEmitOnError": false,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"isolatedModules": true,
		"incremental": false,
		"esModuleInterop": true,
		"downlevelIteration": true
	},
	"include": ["./**/*.ts"]
}
