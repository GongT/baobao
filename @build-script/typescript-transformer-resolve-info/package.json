{
	"name": "@build-script/typescript-transformer-resolve-info",
	"description": "A TypeScript transformer for use with ttypescript that will save import/export information to json file.",
	"version": "0.1.11",
	"license": "MIT",
	"type": "module",
	"main": "./lib/register.cjs",
	"module": "./lib/register.js",
	"exports": {
		".": {
			"require": "./lib/register.cjs",
			"import": "./lib/register.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"lint": "heft build",
		"build": "heft build --production",
		"build:watch": "heft build --lite",
		"watch": "heft build --watch",
		"clean": "heft clean"
	},
	"dependencies": {
		"@idlebox/node": "workspace:*",
		"fancy-log": "^2.0.0",
		"fs-extra": "^10.1.0",
		"source-map-support": "^0.5.21",
		"tslib": "^2.4.0"
	},
	"peerDependencies": {
		"ttypescript": "*",
		"typescript": ">=3.8.0-dev.20200115"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:*1.0.19",
		"@build-script/typescript-transformer-common": "workspace:*0.1.10",
		"@build-script/typescript-transformer-dual-package": "workspace:*1.0.27",
		"@types/fancy-log": "^1.3.1",
		"@types/fs-extra": "^9.0.13",
		"@types/gulp": "^4.0.9",
		"@types/mocha": "^9.1.1",
		"@types/node": "^18.0.3",
		"gulp": "^4.0.2",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"ts-node": "^10.8.2"
	},
	"repository": "https://github.com/GongT/baobao"
}
