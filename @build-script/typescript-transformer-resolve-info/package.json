{
	"name": "@build-script/typescript-transformer-resolve-info",
	"description": "A TypeScript transformer for use with ttypescript that will save import/export information to json file.",
	"version": "0.0.1",
	"license": "WTFPL",
	"type": "module",
	"main": "./lib/register.cjs",
	"module": "./lib/register.js",
	"exports": {
		".": {
			"require": "./lib/register.cjs",
			"import": "./lib/register.js"
		}
	},
	"scripts": {
		"prepack": "rimraf output && ttsc -p src",
		"build": "ttsc -p src",
		"watch": "ttsc -p src -w",
		"fancy-log": "^1.3.3",
		"fs-extra": "^9.0.1",
		"clean": "rimraf lib",
		"test": "ttsc -p src"
	},
	"dependencies": {
		"@build-script/typescript-transformer-common": "^0.0.1",
		"@idlebox/node": "^1.0.12",
		"fancy-log": "^1.3.3",
		"fs-extra": "^9.0.1",
		"source-map-support": "^0.5.19"
	},
	"peerDependencies": {
		"ttypescript": "*",
		"typescript": ">=3.8.0-dev.20200115"
	},
	"devDependencies": {
		"@build-script/typescript-transformer-dual-package": "^1.0.10",
		"@build-script/single-dog-asset": "^1.0.3",
		"@types/fancy-log": "^1.3.1",
		"@types/fs-extra": "^9.0.1",
		"@types/node": "^14.6.0",
		"rimraf": "^3.0.2",
		"ts-node": "^9.0.0",
		"ttypescript": "^1.5.11",
		"typescript": "^4.0.2",
		"tslib": "^2.0.1"
	},
	"repository": "https://github.com/GongT/baobao"
}
