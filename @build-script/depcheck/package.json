{
	"name": "@build-script/depcheck",
	"type": "module",
	"version": "0.0.3",
	"keywords": ["depcheck"],
	"bin": {
		"depcheck": "./loader/bin.devel.js"
	},
	"exports": {
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepublishHook": "internal-prepublish-hook",
		"prepack": "mpis-run build --clean",
		"build": "mpis-run build",
		"watch": "mpis-run watch",
		"clean": "mpis-run clean",
		"lint": "internal-lint",
		"prepublishOnly": "internal-prepublish-deny"
	},
	"dependencies": {
		"@build-script/rushstack-config-loader": "workspace:^",
		"@idlebox/cli": "workspace:^",
		"@idlebox/chokidar": "workspace:^",
		"@idlebox/common": "workspace:^",
		"@idlebox/node": "workspace:^",
		"@idlebox/source-map-support": "workspace:^",
		"@mpis/client": "workspace:^",
		"depcheck": "^1.4.7",
		"tslib": "^2.8.1"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@idlebox/esbuild-executer": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@mpis/run": "workspace:^",
		"@types/node": "^24.0.14",
		"typescript": "^5.8.3"
	},
	"publishConfig": {
		"packCommand": ["publisher", "pack"]
	},
	"sideEffects": true
}
