{
	"name": "@build-script/typescript-transformer-import-commonjs",
	"description": "A TypeScript transformer for use with ttypescript that will change import statement of commonjs packages.",
	"version": "1.0.20",
	"license": "MIT",
	"type": "module",
	"main": "./lib/register.cjs",
	"module": "./lib/register.js",
	"exports": {
		".": {
			"require": "./lib/register.cjs",
			"import": "./lib/register.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "heft build --production",
		"build:watch": "heft build --lite",
		"watch": "heft build --watch",
		"clean": "heft clean"
	},
	"peerDependencies": {
		"ttypescript": "*",
		"typescript": ">=3.8.0-dev.20200115"
	},
	"devDependencies": {
		"@rushstack/heft": "^0.48.8",
		"@build-script/typescript-transformer-dual-package": "workspace:*1.0.27",
		"@build-script/single-dog-asset": "workspace:*1.0.19",
		"@types/node": "^18.0.3",
		"rimraf": "^3.0.2",
		"ttypescript": "^1.5.13",
		"typescript": "^4.7.4",
		"npm-run-all": "^4.1.5"
	},
	"repository": "https://github.com/GongT/baobao",
	"dependencies": {
		"tslib": "^2.4.0"
	}
}
