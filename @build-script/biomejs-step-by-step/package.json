{
	"name": "@build-script/biomejs-step-by-step",
	"type": "module",
	"private": true,
	"version": "1.0.11",
	"keywords": ["biomejs", "biome", "step-by-step", "interactive"],
	"bin": {
		"codegen": "./loader/bin.js"
	},
	"exports": {
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepublishHook": "internal-prepublish-hook",
		"prepack": "mpis-run build --clean",
		"build": "mpis-run build",
		"watch": "mpis-run watch",
		"clean": "mpis-run clean",
		"lint": "internal-lint",
		"prepublishOnly": "internal-prepublish-deny"
	},
	"dependencies": {
		"@biomejs/js-api": "^4.0.0",
		"@biomejs/wasm-nodejs": "^2.3.11",
		"@idlebox/json-edit": "workspace:^",
		"@idlebox/chokidar": "workspace:^",
		"@idlebox/cli": "workspace:^",
		"@idlebox/common": "workspace:^",
		"@idlebox/node": "workspace:^",
		"@idlebox/source-map-support": "workspace:^",
		"execa": "^9.6.1"
	},
	"devDependencies": {
		"@build-script/baseline-rig": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@types/node": "^25.0.8",
		"typescript": "^5.9.3"
	},
	"publishConfig": {
		"packCommand": ["publisher", "pack"]
	},
	"peerDependencies": {
		"@biomejs/biome": ">=2.2.2"
	},
	"sideEffects": false
}
