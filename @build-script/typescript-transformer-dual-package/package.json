{
	"name": "@build-script/typescript-transformer-dual-package",
	"description": "A TypeScript transformer for (mainly) use with ttypescript that will (semi-)auto create dual package hazard.",
	"keywords": [
		"ttypescript",
		"typescript",
		"transformer",
		"dual package",
		"esm",
		"es modules",
		"commonjs"
	],
	"version": "1.0.12",
	"license": "WTFPL",
	"type": "module",
	"main": "./lib/register.cjs",
	"typings": "./lib/register.d.ts",
	"module": "./lib/register.js",
	"exports": {
		".": {
			"require": "./lib/register.cjs",
			"import": "./lib/register.js"
		}
	},
	"scripts": {
		"prepack": "rimraf output && ttsc -p src",
		"build": "ttsc -p src",
		"watch": "ttsc -p src -w",
		"clean": "rimraf lib",
		"test": ""
	},
	"peerDependencies": {
		"ttypescript": "*",
		"typescript": ">=3.8.0-dev.20200115"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "^1.0.5",
		"@types/node": "^14.10.1",
		"rimraf": "^3.0.2",
		"ts-node": "^9.0.0",
		"ttypescript": "^1.5.12",
		"typescript": "^4.0.2",
		"tslib": "^2.0.1"
	},
	"repository": "https://github.com/GongT/baobao"
}
