{
	"name": "@build-script/typescript-transformer-static-resolve",
	"description": "A TypeScript transformer for use with ttypescript that will resolve import target during transpile.",
	"version": "0.0.3",
	"license": "MIT",
	"type": "module",
	"main": "./lib/register.cjs",
	"module": "./lib/register.js",
	"exports": {
		".": {
			"require": "./lib/register.cjs",
			"import": "./lib/register.js"
		}
	},
	"scripts": {
		"prepack": "rimraf output && ttsc -p src",
		"build": "ttsc -p src",
		"watch": "ttsc -p src -w",
		"clean": "rimraf lib",
		"test": ""
	},
	"dependencies": {
		"source-map-support": "^0.5.19",
		"@build-script/typescript-transformer-common": "^0.0.3",
		"fancy-log": "^1.3.3",
		"fs-extra": "^9.0.1"
	},
	"peerDependencies": {
		"ttypescript": "*",
		"typescript": ">=3.8.0-dev.20200115"
	},
	"devDependencies": {
		"@build-script/typescript-transformer-dual-package": "^1.0.13",
		"@build-script/single-dog-asset": "^1.0.5",
		"@types/fancy-log": "^1.3.1",
		"@types/fs-extra": "^9.0.1",
		"@types/node": "^14.10.2",
		"rimraf": "^3.0.2",
		"ts-node": "^9.0.0",
		"ttypescript": "^1.5.12",
		"typescript": "^4.0.2",
		"tslib": "^2.0.1"
	},
	"repository": "https://github.com/GongT/baobao"
}
