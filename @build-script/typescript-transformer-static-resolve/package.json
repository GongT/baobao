{
	"name": "@build-script/typescript-transformer-static-resolve",
	"description": "A TypeScript transformer for use with ttypescript that will resolve import target during transpile.",
	"version": "0.0.13",
	"license": "MIT",
	"type": "module",
	"main": "./lib/transform.cjs",
	"module": "./lib/transform.js",
	"exports": {
		".": {
			"require": "./lib/transform.cjs",
			"import": "./lib/transform.js"
		}
	},
	"scripts": {
		"prepack": "run-s clean build",
		"build": "ttsc -p src",
		"watch": "ttsc -p src -w",
		"clean": "rimraf lib",
		"test": ""
	},
	"dependencies": {
		"source-map-support": "^0.5.21",
		"@build-script/typescript-transformer-common": "^0.1.8",
		"fancy-log": "^2.0.0",
		"fs-extra": "^10.0.1",
		"tslib": "^2.3.1"
	},
	"peerDependencies": {
		"ttypescript": "*",
		"typescript": ">=3.8.0-dev.20200115"
	},
	"devDependencies": {
		"@build-script/typescript-transformer-dual-package": "^1.0.25",
		"@build-script/single-dog-asset": "^1.0.16",
		"@types/fancy-log": "^1.3.1",
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^17.0.21",
		"rimraf": "^3.0.2",
		"ttypescript": "^1.5.13",
		"typescript": "^4.6.2",
		"npm-run-all": "^4.1.5",
		"ts-node": "^10.7.0"
	},
	"repository": "https://github.com/GongT/baobao"
}
