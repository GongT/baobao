{
	"monorepo": "rush",
	"type": "module",
	"name": "@build-script/builder",
	"description": "run build command(s) serial/parallel with order. (by generate a Gulp config)",
	"keywords": ["gulp", "build", "process", "tool", "make", "pipeline"],
	"version": "2.1.2",
	"main": "./api.cjs",
	"module": "./api.js",
	"typings": "./api.d.ts",
	"exports": {
		".": {
			"require": "./api.cjs",
			"import": "./api.js"
		}
	},
	"bin": {
		"build-script": "bin/multi-call.cjs",
		"rimraf": "bin/rimraf.cjs"
	},
	"preferGlobal": true,
	"license": "MIT",
	"scripts": {
		"clean": "rimraf lib",
		"watch": "ttsc -p src -w",
		"build": "ttsc -p src",
		"prepack": "run-s -l clean build"
	},
	"dependencies": {
		"@idlebox/common": "^1.1.0",
		"@idlebox/node": "^1.0.24",
		"@idlebox/node-json-edit": "^2.0.10",
		"@build-script/dual-package-runtime": "^1.0.6",
		"execa": "^4.0.3",
		"fancy-log": "^1.3.3",
		"fs-extra": "^9.0.1",
		"npm-check-updates": "^9.1.2",
		"pacote": "^11.1.11",
		"source-map-support": "^0.5.19",
		"split2": "^3.2.2"
	},
	"devDependencies": {
		"@build-script/typescript-transformer-dual-package": "^1.0.16",
		"@build-script/single-dog-asset": "^1.0.9",
		"@microsoft/api-extractor": "^7.10.4",
		"@types/fancy-log": "^1.3.1",
		"@types/fs-extra": "^9.0.2",
		"@types/gulp": "^4.0.7",
		"@types/node": "^14.14.0",
		"@types/split2": "^2.1.6",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"tslib": "^2.0.3",
		"ttypescript": "^1.5.12",
		"typescript": "^4.0.3"
	},
	"repository": "https://github.com/GongT/baobao"
}
