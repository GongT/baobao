{
	"monorepo": "rush",
	"type": "commonjs",
	"name": "@build-script/builder",
	"description": "run build command(s) serial/parallel with order. (by generate a Gulp config)",
	"keywords": ["gulp", "build", "process", "tool", "make", "pipeline"],
	"version": "3.1.3",
	"main": "./lib/api/context.js",
	"typings": "./api.d.ts",
	"bin": {
		"build-script": "bin/multi-call.cjs",
		"rimraf": "bin/rimraf.cjs"
	},
	"preferGlobal": true,
	"license": "MIT",
	"scripts": {
		"clean": "rimraf lib",
		"watch": "tsc -p src -w",
		"build": "tsc -p src",
		"prepack": "run-s -l clean build"
	},
	"dependencies": {
		"@idlebox/common": "^1.2.5",
		"@idlebox/node": "^1.1.1",
		"@idlebox/node-json-edit": "^2.0.16",
		"@build-script/gulp-chain-simplify": "^0.0.6",
		"execa": "^5.1.1",
		"fancy-log": "^1.3.3",
		"fs-extra": "^10.0.0",
		"npm-check-updates": "^11.8.5",
		"pacote": "^12.0.0",
		"source-map-support": "^0.5.20",
		"split2": "^3.2.2",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "^1.0.14",
		"@microsoft/api-extractor": "^7.18.15",
		"@types/fancy-log": "^1.3.1",
		"@types/fs-extra": "^9.0.13",
		"@types/gulp": "^4.0.9",
		"@types/node": "^16.10.3",
		"@types/split2": "^3.2.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.4.3"
	},
	"repository": "https://github.com/GongT/baobao"
}
