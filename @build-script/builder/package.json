{
	"name": "@build-script/builder",
	"type": "commonjs",
	"version": "3.2.9",
	"description": "run build command(s) serial/parallel with order. (by generate a Gulp config)",
	"keywords": ["build", "gulp", "make", "pipeline", "process", "tool"],
	"license": "MIT",
	"main": "./bin/api.cjs",
	"bin": {
		"build-script": "bin/multi-call.cjs",
		"rimraf": "bin/rimraf.cjs"
	},
	"typings": "./bin/api.d.ts",
	"scripts": {
		"prepack": "run-s -l clean build",
		"clean": "rimraf lib dist .rush temp",
		"watch": "tsc -p src -w",
		"build": "tsc -p src"
	},
	"repository": "https://github.com/GongT/baobao",
	"preferGlobal": true,
	"dependencies": {
		"@build-script/gulp-chain-simplify": "workspace:^",
		"@gongt/fix-esm": "workspace:^",
		"@idlebox/common": "workspace:*",
		"@idlebox/node": "workspace:^",
		"@idlebox/node-json-edit": "workspace:^",
		"execa": "^6.1.0",
		"fancy-log": "^2.0.0",
		"fs-extra": "^10.1.0",
		"npm-check-updates": "^16.3.11",
		"pacote": "^15.0.0",
		"source-map-support": "^0.5.21",
		"split2": "^4.1.0",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@microsoft/api-extractor": "^7.33.2",
		"@types/fancy-log": "^2.0.0",
		"@types/fs-extra": "^9.0.13",
		"@types/gulp": "^4.0.9",
		"@types/node": "^18.11.0",
		"@types/split2": "^3.2.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.8.4"
	},
	"monorepo": "rush"
}
