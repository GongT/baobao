{
	"monorepo": "rush",
	"type": "commonjs",
	"name": "@build-script/builder",
	"description": "run build command(s) serial/parallel with order. (by generate a Gulp config)",
	"keywords": ["gulp", "build", "process", "tool", "make", "pipeline"],
	"version": "3.2.2",
	"main": "./bin/api.cjs",
	"typings": "./bin/api.d.ts",
	"bin": {
		"build-script": "bin/multi-call.cjs",
		"rimraf": "bin/rimraf.cjs"
	},
	"preferGlobal": true,
	"license": "MIT",
	"scripts": {
		"clean": "rimraf lib",
		"watch": "tsc -p src -w",
		"build": "tsc -p src",
		"prepack": "run-s -l clean build"
	},
	"dependencies": {
		"@build-script/gulp-chain-simplify": "^0.0.7",
		"@idlebox/common": "^1.3.1",
		"@idlebox/node": "^1.2.1",
		"@idlebox/node-json-edit": "^2.0.17",
		"esm": "^3.2.25",
		"execa": "^6.0.0",
		"fancy-log": "^1.3.3",
		"fix-esm": "^1.0.1",
		"fs-extra": "^10.0.0",
		"npm-check-updates": "^12.0.2",
		"pacote": "^12.0.2",
		"source-map-support": "^0.5.21",
		"split2": "^4.1.0",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "^1.0.14",
		"@microsoft/api-extractor": "^7.18.19",
		"@types/fancy-log": "^1.3.1",
		"@types/fs-extra": "^9.0.13",
		"@types/gulp": "^4.0.9",
		"@types/node": "^16.11.10",
		"@types/split2": "^3.2.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.5.2"
	},
	"repository": "https://github.com/GongT/baobao"
}
