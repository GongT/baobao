{
	"monorepo": "rush",
	"type": "commonjs",
	"name": "@build-script/builder",
	"description": "run build command(s) serial/parallel with order. (by generate a Gulp config)",
	"keywords": ["gulp", "build", "process", "tool", "make", "pipeline"],
	"version": "3.2.5",
	"main": "./bin/api.cjs",
	"typings": "./bin/api.d.ts",
	"bin": {
		"build-script": "bin/multi-call.cjs",
		"rimraf": "bin/rimraf.cjs"
	},
	"preferGlobal": true,
	"license": "MIT",
	"scripts": {
		"clean": "rimraf lib",
		"watch": "tsc -p src -w",
		"build": "tsc -p src",
		"prepack": "run-s -l clean build"
	},
	"dependencies": {
		"@build-script/gulp-chain-simplify": "workspace:^0.0.10",
		"@idlebox/common": "workspace:^1.3.8",
		"@idlebox/node": "workspace:^1.2.5",
		"@idlebox/node-json-edit": "workspace:^2.0.20",
		"execa": "^6.1.0",
		"fancy-log": "^2.0.0",
		"@gongt/fix-esm": "workspace:^0.0.5",
		"fs-extra": "^10.1.0",
		"npm-check-updates": "^14.0.2",
		"pacote": "^13.6.1",
		"source-map-support": "^0.5.21",
		"split2": "^4.1.0",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^1.0.18",
		"@microsoft/api-extractor": "^7.25.2",
		"@types/fancy-log": "^1.3.1",
		"@types/fs-extra": "^9.0.13",
		"@types/gulp": "^4.0.9",
		"@types/node": "^18.0.0",
		"@types/split2": "^3.2.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.4"
	},
	"repository": "https://github.com/GongT/baobao"
}
