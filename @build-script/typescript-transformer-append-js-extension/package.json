{
	"name": "@build-script/typescript-transformer-append-js-extension",
	"description": "A TypeScript transformer append '.js' in each import path.",
	"keywords": ["ttypescript", "typescript", "transformer"],
	"version": "1.0.5",
	"license": "MIT",
	"type": "module",
	"main": "./lib/register.cjs",
	"typings": "./lib/register.d.ts",
	"module": "./lib/register.js",
	"exports": {
		".": {
			"require": "./lib/register.cjs",
			"import": "./lib/register.js"
		}
	},
	"scripts": {
		"prepack": "run-s clean build",
		"build": "ttsc -p src",
		"watch": "ttsc -p src -w",
		"clean": "rimraf lib",
		"test": ""
	},
	"peerDependencies": {
		"ttypescript": "*"
	},
	"devDependencies": {
		"@build-script/typescript-transformer-dual-package": "^1.0.22",
		"@build-script/single-dog-asset": "^1.0.14",
		"@types/node": "^16.10.3",
		"rimraf": "^3.0.2",
		"esm": "^3.2.25",
		"ttypescript": "^1.5.12",
		"typescript": "^4.4.3",
		"npm-run-all": "^4.1.5"
	},
	"repository": "https://github.com/GongT/baobao",
	"dependencies": {
		"tslib": "^2.3.1"
	}
}
