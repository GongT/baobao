{
	"name": "@build-script/typescript-transformer-append-js-extension",
	"description": "A TypeScript transformer append '.js' in each import path.",
	"keywords": ["ttypescript", "typescript", "transformer"],
	"version": "1.0.1",
	"license": "MIT",
	"type": "module",
	"main": "./lib/register.cjs",
	"typings": "./lib/register.d.ts",
	"module": "./lib/register.js",
	"exports": {
		".": {
			"require": "./lib/register.cjs",
			"import": "./lib/register.js"
		}
	},
	"scripts": {
		"prepack": "rimraf output && ttsc -p src",
		"build": "ttsc -p src",
		"watch": "ttsc -p src -w",
		"clean": "rimraf lib",
		"test": ""
	},
	"peerDependencies": {
		"ttypescript": "*"
	},
	"devDependencies": {
		"@build-script/typescript-transformer-dual-package": "^1.0.18",
		"@build-script/single-dog-asset": "^1.0.10",
		"@types/node": "^14.14.14",
		"rimraf": "^3.0.2",
		"ts-node": "^9.1.1",
		"ttypescript": "^1.5.12",
		"typescript": "^4.1.3",
		"tslib": "^2.0.3"
	},
	"repository": "https://github.com/GongT/baobao"
}
