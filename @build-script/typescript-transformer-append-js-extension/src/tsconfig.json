{
	"extends": "../node_modules/@build-script/single-dog-asset/package/tsconfig.json",
	"compilerOptions": {
		"module": "ESNext",
		"declaration": true,
		"declarationMap": true,
		"outDir": "../lib",
		"rootDir": "./",
		"typeRoots": ["../node_modules/@types"],
		"types": ["node"],
		"plugins": [
			{
				"transform": "../lib/register.js"
			},
			{
				"transform": "@build-script/typescript-transformer-dual-package"
			}
		],
		"incremental": false
	},
	"include": ["**/*.ts"]
}
