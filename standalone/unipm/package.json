{
	"monorepo": true,
	"name": "unipm",
	"version": "1.0.3",
	"type": "module",
	"main": "./lib/api.cjs",
	"typings": "./lib/api.d.ts",
	"bin": {
		"unipm": "./bin.cjs"
	},
	"exports": {
		".": {
			"import": "./lib/api.js",
			"require": "./lib/api.cjs"
		}
	},
	"scripts": {
		"build": "tsc -p src",
		"clean": "rimraf lib",
		"prepack": "rimraf lib && npm run build",
		"watch": "tsc -w -p src"
	},
	"preferGlobal": true,
	"dependencies": {
		"@idlebox/common": "^1.0.16",
		"@idlebox/node": "^1.0.7",
		"execa": "^4.0.3",
		"json5": "^2.1.3",
		"pacote": "^11.1.10",
		"source-map-support": "^0.5.19"
	},
	"devDependencies": {
		"@idlebox/node-json-edit": "^1.0.20",
		"@build-script/typescript-transformer-dual-package": "^1.0.3",
		"@build-script/single-dog-asset": "^1.0.2",
		"@types/json5": "^0.0.30",
		"@types/node": "^14.0.19",
		"npm-check-updates": "^7.0.1",
		"rimraf": "^3.0.2",
		"typescript": "^3.9.6",
		"tslib": "^2.0.0",
		"ttypescript": "^1.5.10"
	},
	"repository": "https://github.com/GongT/baobao",
	"license": "MIT"
}
