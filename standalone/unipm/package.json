{
	"monorepo": true,
	"name": "unipm",
	"version": "1.0.3",
	"type": "module",
	"main": "./lib/api.cjs",
	"typings": "./lib/api.d.ts",
	"bin": {
		"unipm": "./bin.cjs"
	},
	"exports": {
		".": {
			"import": "./lib/api.js",
			"require": "./lib/api.cjs"
		}
	},
	"scripts": {
		"build": "tsc -p src",
		"clean": "rimraf lib",
		"prepack": "rimraf lib && npm run build",
		"upgrade": "ncu --upgrade --packageFile ./package.json",
		"watch": "tsc -w -p src"
	},
	"preferGlobal": true,
	"dependencies": {
		"@idlebox/common": "^1.0.8",
		"@idlebox/node": "^1.0.5",
		"execa": "^2.0.4",
		"json5": "^2.1.0",
		"pacote": "^9.5.8",
		"source-map-support": "*"
	},
	"devDependencies": {
		"@idlebox/node-json-edit": "^1.0.19",
		"@build-script/typescript-transformer-dual-package": "^1.0.3",
		"@build-script/single-dog-asset": "^1.0.2",
		"@types/json5": "^0.0.30",
		"@types/node": "^12.7.8",
		"npm-check-updates": "^3.1.23",
		"rimraf": "^3.0.0",
		"typescript": "^3.8.2",
		"tslib": "^1.10.0",
		"ttypescript": "^1.5.10"
	},
	"repository": "https://github.com/GongT/baobao"
}
