{
	"name": "cjke-strings",
	"type": "module",
	"version": "1.1.0",
	"description": "a set of Chinese Japanese Korean and Emoji strings helpers",
	"keywords": ["CJK", "chinese", "display width", "emoji", "string", "string width", "unicode"],
	"license": "MIT",
	"main": "./lib/cjs/x-index.cjs",
	"module": "./lib/esm/x-index.js",
	"types": "./lib/esm/x-index.d.ts",
	"exports": {
		".": {
			"source": "./src/x-index.ts",
			"types": "./lib/esm/x-index.d.ts",
			"import": "./lib/esm/x-index.js",
			"require": "./lib/cjs/x-index.cjs"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepack": "heft --unmanaged build --production --clean",
		"test": "heft test",
		"build": "heft --unmanaged build",
		"_phase:build": "heft --unmanaged build",
		"watch": "heft --unmanaged build-watch",
		"clean": "heft clean"
	},
	"repository": "https://github.com/GongT/cjke.git",
	"dependencies": {
		"ansi-regex": "^6.1.0",
		"emoji-regex": "^10.4.0",
		"is-fullwidth-code-point": "^5.0.0",
		"source-map-support": "^0.5.21"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@idlebox/itypes": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@rushstack/heft": "^0.73.6",
		"@types/chai": "^5.2.2",
		"@types/mocha": "^10.0.10",
		"chai": "^5.2.0",
		"mocha": "^11.5.0"
	}
}
