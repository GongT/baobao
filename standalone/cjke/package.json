{
	"name": "cjke-strings",
	"type": "module",
	"version": "1.1.11",
	"description": "a set of Chinese Japanese Korean and Emoji strings helpers",
	"keywords": ["CJK", "chinese", "display width", "emoji", "string", "string width", "unicode"],
	"exports": {
		".": {
			"source": "./src/autoindex.ts",
			"default": "./lib/autoindex.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepublishHook": "internal-prepublish-hook",
		"prepack": "mpis-run build --clean",
		"build": "mpis-run build",
		"watch": "mpis-run watch",
		"clean": "mpis-run clean",
		"lint": "internal-lint",
		"prepublishOnly": "internal-prepublish-deny"
	},
	"dependencies": {
		"ansi-regex": "^6.1.0",
		"emoji-regex": "^10.4.0",
		"is-fullwidth-code-point": "^5.0.0"
	},
	"devDependencies": {
		"@idlebox/source-map-support": "workspace:^",
		"@build-script/single-dog-asset": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@mpis/run": "workspace:^",
		"@types/chai": "^5.2.2",
		"@types/mocha": "^10.0.10",
		"chai": "^5.2.1",
		"mocha": "^11.7.1"
	},
	"decoupledDependencies": "dev",
	"publishConfig": {
		"packCommand": ["publisher", "pack"]
	},
	"sideEffects": false
}
