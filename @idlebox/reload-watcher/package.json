{
	"name": "@idlebox/reload-watcher",
	"type": "module",
	"private": true,
	"version": "0.0.1",
	"description": "watch changes to imported module, and skip cache next import.",
	"keywords": ["comment", "edit", "json", "jsonc"],
	"license": "MIT",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"typescript": "./src/index.ts",
			"types": "./lib/index.d.ts",
			"default": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"sideEffects": false,
	"scripts": {
		"prepack": "heft --unmanaged build --production --clean",
		"build": "heft --unmanaged build",
		"_phase:build": "heft --unmanaged build",
		"watch": "heft --unmanaged build-watch",
		"clean": "heft clean"
	},
	"repository": "https://github.com/GongT/baobao",
	"dependencies": {
		"@idlebox/common": "workspace:^",
		"chokidar": "^4.0.1"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@rushstack/heft": "^0.68.2",
		"@types/node": "^22.8.6",
		"source-map-support": "^0.5.21",
		"tslib": "^2.7.0"
	}
}
