{
	"name": "@idlebox/json-edit-biome",
	"type": "module",
	"version": "0.0.6",
	"description": "biomejs formatter for @idlebox/json-edit.",
	"keywords": ["comment", "edit", "json", "jsonc"],
	"exports": {
		".": {
			"source": "./src/index.ts",
			"types": "./src/index.ts",
			"default": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"sideEffects": false,
	"scripts": {
		"prepublishHook": "internal-prepublish-hook",
		"prepack": "mpis-run build --clean",
		"build": "mpis-run build",
		"watch": "mpis-run watch",
		"clean": "mpis-run clean",
		"lint": "internal-lint",
		"prepublishOnly": "internal-prepublish-deny"
	},
	"dependencies": {
		"comment-json": "^4.2.5",
		"prettier": "^3.6.2"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@mpis/run": "workspace:^",
		"@rushstack/heft": "^0.74.0",
		"@types/node": "^24.0.14",
		"source-map-support": "^0.5.21",
		"tslib": "^2.8.1"
	},
	"publishConfig": {
		"packCommand": ["publisher", "pack"]
	}
}
