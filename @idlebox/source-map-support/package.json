{
	"name": "@idlebox/source-map-support",
	"type": "module",
	"version": "0.0.13",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"source": "./src/index.ts",
			"default": "./lib/index.js"
		},
		"./register": {
			"types": "./src/register.ts",
			"source": "./src/register.ts",
			"default": "./lib/register.js"
		},
		"./register.js": "./lib/register.js",
		"./package.json": "./package.json"
	},
	"sideEffects": ["./lib/register.js"],
	"scripts": {
		"prepublishHook": "internal-prepublish-hook",
		"prepack": "mpis-run build --clean",
		"build": "mpis-run build",
		"watch": "mpis-run watch",
		"clean": "mpis-run clean",
		"lint": "internal-lint",
		"prepublishOnly": "internal-prepublish-deny"
	},
	"dependencies": {
		"@types/source-map-support": "^0.5.10",
		"@idlebox/common": "workspace:^",
		"source-map-support": "^0.5.21"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@types/node": "^24.0.14"
	},
	"publishConfig": {
		"packCommand": ["publisher", "pack"]
	}
}
