{
	"name": "@idlebox/esbuild-executer",
	"description": "A simple script to execute typescript file during development.",
	"version": "0.0.1",
	"type": "module",
	"main": "./lib/index.js",
	"bin": {
		"es-node": "./lib/bin.js"
	},
	"engines": {
		"node": ">=18.19.0 < 20 || >=20.6.0"
	},
	"exports": {
		".": "./lib/index.js",
		"./register": "./lib/hook-worker.js",
		"./lib/hook-worker.js": "./lib/hook-worker.js",
		"./package.json": "./package.json"
	},
	"keywords": ["typescript", "esbuild", "executer", "runner"],
	"license": "MIT",
	"scripts": {
		"build": "tsc -p src",
		"watch": "tsc -p src -w"
	},
	"dependencies": {
		"debug": "^4.4.1",
		"esbuild": "^0.25.5",
		"source-map-support": "^0.5.21",
		"tslib": "^2.8.1"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@types/node": "^22.15.21",
		"typescript": "^5.8.3"
	}
}
