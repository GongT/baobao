{
	"name": "@idlebox/common",
	"type": "module",
	"version": "1.4.14",
	"description": "Common utilities",
	"keywords": [
		"utilities",
		"functions",
		"helpers",
		"library"
	],
	"exports": {
		".": {
			"types": "./src/autoindex.ts",
			"source": "./src/autoindex.ts",
			"default": "./lib/autoindex.js"
		},
		"./package.json": "./package.json"
	},
	"sideEffects": false,
	"scripts": {
		"prepublishHook": "internal-prepublish-hook",
		"prepack": "mpis-run build --clean",
