{
	"$schema": "../node_modules/@mpis/run/commands.schema.json",
	"build": [
		// "bash -c 'echo aaaaaaaaaaaaaaaaaaaaa'",
		/**
		 *
		 */
		"autoindex -o autoindex src",

		/**
		 *
		 */
		"codegen src",

		/**
		 *
		 */
		{
			"title": "Compile TypeScript",
			"command": ["mpis-tsc", "-p", "src"],
			"watch": ["-w"],
			"cwd": ".",
			"env": {}
		}
	],
	"clean": ["lib"]
}
