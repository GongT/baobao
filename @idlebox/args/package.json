{
	"name": "@idlebox/args",
	"type": "module",
	"version": "0.0.19",
	"exports": {
		".": {
			"source": "./src/interface.ts",
			"types": "./src/interface.ts",
			"default": "./lib/interface.js"
		},
		"./default": {
			"source": "./src/default.ts",
			"types": "./src/default.ts",
			"default": "./lib/default.js"
		},
		"./package.json": "./package.json"
	},
	"sideEffects": false,
	"scripts": {
		"prepack": "mpis-run build --clean",
		"build": "mpis-run build",
		"watch": "mpis-run watch",
		"clean": "mpis-run clean",
		"test": "mocha --require @idlebox/source-map-support/register --bail lib/tests",
		"prepublishHook": "internal-prepublish-hook",
		"lint": "internal-lint",
		"prepublishOnly": "internal-prepublish-deny"
	},
	"dependencies": {
		"@idlebox/errors": "workspace:^",
		"cjke-strings": "workspace:^"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@mpis/run": "workspace:^",
		"@types/chai": "^5.2.2",
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.0.14",
		"chai": "^5.2.1",
		"mocha": "^11.7.1",
		"@idlebox/source-map-support": "workspace:^"
	},
	"publishConfig": {
		"packCommand": ["publisher", "pack"]
	}
}
