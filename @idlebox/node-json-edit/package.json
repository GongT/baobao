{
	"name": "@idlebox/node-json-edit",
	"description": "Edit json file with comments (jsonc), reformat or preserve it's original format.",
	"keywords": ["json", "comment", "jsonc", "edit"],
	"version": "2.0.16",
	"monorepo": true,
	"type": "module",
	"main": "./lib/index.cjs",
	"module": "./lib/index.js",
	"typings": "./docs/package-public.d.ts",
	"exports": {
		".": {
			"require": "./lib/index.cjs",
			"import": "./lib/index.js"
		}
	},
	"scripts": {
		"build": "scripts/build.sh",
		"prepack": "scripts/pack.sh",
		"postpack": "scripts/postpack.sh",
		"watch": "ttsc -p src -w",
		"test": "scripts/test.sh",
		"clean": "rimraf lib"
	},
	"license": "MIT",
	"dependencies": {
		"comment-json": "^4.1.1",
		"prettier": "^2.4.1",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@build-script/typescript-transformer-dual-package": "^1.0.22",
		"@build-script/single-dog-asset": "^1.0.14",
		"@types/node": "^16.10.3",
		"rimraf": "^3.0.2",
		"npm-check-updates": "^11.8.5",
		"source-map-support": "^0.5.20",
		"ttypescript": "^1.5.12",
		"typescript": "^4.4.3",
		"@types/prettier": "^2.4.1"
	},
	"repository": "https://github.com/GongT/baobao"
}
