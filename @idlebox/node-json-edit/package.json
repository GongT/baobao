		"tslib": "^2.4.1"
	},
	"devDependencies": {
		"@build-script/heft-duel-stack": "workspace:*",
		"@build-script/node-import-test": "workspace:*",
		"@build-script/single-dog-asset": "workspace:*",
		"@build-script/typescript-transformer-dual-package": "latest",
		"@rushstack/heft": "^0.48.8",
		"@types/node": "^18.11.0",
		"@types/prettier": "^2.7.1",
		"local-rig": "workspace:*",
		"source-map-support": "^0.5.21",
		"typescript": "^4.8.4"
	},
	"monorepo": true
