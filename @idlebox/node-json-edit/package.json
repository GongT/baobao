{
	"name": "@idlebox/node-json-edit",
	"description": "Edit json file with comments (jsonc), reformat or preserve it's original format.",
	"keywords": [
		"json",
		"comment",
		"jsonc",
		"edit"
	],
	"version": "2.0.7",
	"monorepo": true,
	"type": "module",
	"main": "./lib/index.cjs",
	"module": "./lib/index.js",
	"typings": "./docs/package-public.d.ts",
	"exports": {
		".": {
			"require": "./lib/index.cjs",
			"import": "./lib/index.js"
		}
	},
	"scripts": {
		"build": "scripts/build.sh",
		"prepack": "scripts/pack.sh",
		"postpack": "scripts/postpack.sh",
		"watch": "ttsc -p src -w",
		"test": "scripts/test.sh"
	},
	"license": "MIT",
	"dependencies": {
		"comment-json": "^4.0.6",
		"prettier": "^2.1.2"
	},
	"devDependencies": {
		"@build-script/typescript-transformer-dual-package": "^1.0.13",
		"@types/comment-json": "^1.1.1",
		"@build-script/single-dog-asset": "^1.0.5",
		"@types/node": "^14.10.2",
		"npm-check-updates": "^9.0.1",
		"source-map-support": "^0.5.19",
		"tslib": "^2.0.1",
		"ttypescript": "^1.5.12",
		"typescript": "^4.0.2",
		"@types/prettier": "^2.1.1"
	},
	"repository": "https://github.com/GongT/baobao"
}
