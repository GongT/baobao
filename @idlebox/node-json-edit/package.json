{
	"name": "@idlebox/node-json-edit",
	"description": "Edit json file with comments (jsonc), reformat or preserve it's original format.",
	"keywords": ["json", "comment", "jsonc", "edit"],
	"version": "2.0.21",
	"monorepo": true,
	"type": "module",
	"main": "./lib/index.cjs",
	"module": "./lib/index.js",
	"typings": "./docs/package-public.d.ts",
	"exports": {
		".": {
			"require": "./lib/index.cjs",
			"import": "./lib/index.js"
		}
	},
	"scripts": {
		"build": "scripts/build.sh",
		"prepack": "scripts/pack.sh",
		"postpack": "scripts/postpack.sh",
		"watch": "ttsc -p src -w",
		"test": "scripts/test.sh",
		"clean": "rimraf lib"
	},
	"license": "MIT",
	"dependencies": {
		"comment-json": "^4.2.2",
		"prettier": "^2.7.1",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@build-script/typescript-transformer-dual-package": "workspace:^1.0.27",
		"@build-script/single-dog-asset": "workspace:^1.0.19",
		"@types/node": "^18.0.0",
		"rimraf": "^3.0.2",
		"npm-check-updates": "^14.0.2",
		"source-map-support": "^0.5.21",
		"ttypescript": "^1.5.13",
		"typescript": "^4.7.4",
		"@types/prettier": "^2.6.3"
	},
	"repository": "https://github.com/GongT/baobao"
}
