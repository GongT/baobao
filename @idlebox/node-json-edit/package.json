{
	"name": "@idlebox/node-json-edit",
	"type": "module",
	"version": "2.0.23",
	"description": "Edit json file with comments (jsonc), reformat or preserve it's original format.",
	"keywords": ["comment", "edit", "json", "jsonc"],
	"license": "MIT",
	"main": "./lib/index.cjs",
	"module": "./lib/index.js",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"require": "./lib/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"typings": "./lib/node-json-edit.d.ts",
	"scripts": {
		"build": "heft build --production",
		"build:watch": "heft build --lite",
		"watch": "heft build --watch",
		"clean": "heft clean"
	},
	"repository": "https://github.com/GongT/baobao",
	"dependencies": {
		"comment-json": "^4.2.3",
