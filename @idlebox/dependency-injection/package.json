{
	"name": "@idlebox/dependency-injection",
	"type": "module",
	"version": "0.0.4",
	"description": "manual dependency injection",
	"keywords": ["di", "dependency injection"],
	"exports": {
		".": {
			"source": "./src/index.ts",
			"default": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepublishHook": "internal-prepublish-hook",
		"prepack": "mpis-run build --clean",
		"build": "mpis-run build",
		"watch": "mpis-run watch",
		"clean": "mpis-run clean",
		"lint": "internal-lint",
		"prepublishOnly": "internal-prepublish-deny"
	},
	"dependencies": {
		"@idlebox/common": "workspace:^",
		"debug": "^4.4.1",
		"tslib": "^2.8.1"
	},
	"devDependencies": {
		"@build-script/single-dog-asset": "workspace:^",
		"@idlebox/esbuild-executer": "workspace:^",
		"@internal/local-rig": "workspace:^",
		"@mpis/run": "workspace:^",
		"@types/debug": "^4.1.12",
		"@types/node": "^24.0.14"
	},
	"publishConfig": {
		"packCommand": ["publisher", "pack"]
	},
	"sideEffects": false
}
