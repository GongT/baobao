{
	"name": "@idlebox/ignore-edit",
	"type": "module",
	"version": "0.0.18",
	"description": "api for modify .*ignore files",
	"keywords": [
		"gitignore",
		"ignorefile"
	],
	"license": "MIT",
	"author": "GongT",
	"main": "./lib/cjs/api.cjs",
